public class PTra06_07 {
	public static void main(String[] args) {

		// for文を使って、以下のような出力になるようにしてください
		/*
		 * 出力結果例
		 * -----------------------------------
		 * □□□□□
		 * ■□□□□
		 * ■■□□□
		 * ■■■□□
		 * ■■■■□

		 * ■■■■■
		 * ■■■■□
		 * ■■■□□
		 * ■■□□□
		 * ■□□□□
		 */
		for(int i = 0; i < 5; i++) {
			for(int k = 0; k < i; k++) {
				System.out.print("■");
			}
			for(int j = 5; j > i; j--) {
				System.out.print("□");
			}
			System.out.println("");
		}

		for(int i = 0; i < 5; i++) {
			for(int k = 0; k < i; k++) {
				System.out.print("□");
			}
			for(int j = 5; j > i; j--) {
				System.out.print("■");
			}
			System.out.println("");
		}

	}
}
