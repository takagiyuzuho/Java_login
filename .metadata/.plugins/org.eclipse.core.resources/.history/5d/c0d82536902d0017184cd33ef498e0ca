public class PTra06_06 {
	public static void main(String[] args) {

		// for文を使って、以下のような出力になるようにしてください
		/*
		 * 出力結果例
		 * -----------------------------------
		 * □□□□□
		 * ■□□□□
		 * ■■□□□
		 * ■■■□□
		 * ■■■■□
		 */

		for(int i = 0; i < 5; i++) {
			for(int j = 5; j > 5; j--) {
				System.out.print("□");
			}
			for(int k = 0; k < 5; k++) {
				System.out.print("■");
			}
		}
	}
}
