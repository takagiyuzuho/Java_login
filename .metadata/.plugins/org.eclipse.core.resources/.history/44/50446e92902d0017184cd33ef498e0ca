public class PTra06_07 {
	public static void main(String[] args) {

		// for文を使って、以下のような出力になるようにしてください
		/*
		 * 出力結果例
		 * -----------------------------------
		 * □□□□□
		 * ■□□□□
		 * ■■□□□
		 * ■■■□□
		 * ■■■■□

		 * ■■■■■
		 * ■■■■□
		 * ■■■□□
		 * ■■□□□
		 * ■□□□□
		 */
		for(int i = 0; i < 5; i++){
			for(int black = 0;  black < i; black++){
				System.out.print("■");
			}
			for(int white = 0;  white < 5 - i; white++){
				System.out.print("□");
			}
			System.out.println();
		}

		for(int i = 0; i < 5; i++){
			for(int white = 0;  white < 5 - i; white++){
				System.out.print("■");
			}
			for(int black = 0;  black < i; black++){
				System.out.print("□");
			}
			System.out.println();
		}

	}
}
