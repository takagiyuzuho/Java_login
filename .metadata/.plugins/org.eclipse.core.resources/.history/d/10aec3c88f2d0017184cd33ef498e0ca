public class PTra06_05 {
	public static void main(String[] args) {

		// for文を使って、以下のような出力になるようにしてください
		/*
		 * 出力結果例
		 * -----------------------------------
		 * ■■■■■
		 * ■■■■■
		 * ■■■■■
		 * ■■■■■
		 * ■■■■■
		 */
		for(int i = 0; i < 5; i++) {
			for(int i = 0; i < 5; i++) {
				System.out.print("■");
			}
		}

	}
}
